    "def f(n):\n",
    "    if n == 1 or n == 0:\n",
    "        return 1\n",
    "    return n * f(n-1)\n",
    "\n",
    "for _ in range(int(input())):\n",
    "    r, n = map(int, input().split())\n",
    "    ans = f(n) // f(r) // f(n-r)\n",
    "    print(ans)"
   ]
