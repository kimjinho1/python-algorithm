    "n = int(input())\n",
    "s = 0\n",
    "li = []\n",
    "for i in range(n):\n",
    "    li.append(int(input()))  \n",
    "    s += li[i] * (-1)**i\n",
    "a = s // 2\n",
    "\n",
    "print(a)\n",
    "for i in range(n-1):\n",
    "    a = li[i] - a\n",
    "    print(a)"
