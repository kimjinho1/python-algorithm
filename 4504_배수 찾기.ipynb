   "source": [
    "n = int(input())\n",
    "while 1:\n",
    "    t = int(input())\n",
    "    if t == 0:\n",
    "        break\n",
    "    if t % n != 0:\n",
    "        print('{0} is NOT a multiple of {1}.'.format(t, n))\n",
    "    else:\n",
    "        print('{0} is a multiple of {1}.'.format(t, n))"
   ]
